<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of Cura 3D printer software as a dmg. From this it extracts Cura.app from the /Cura/ subfolder and copies to a new dmg file by itself. The input key `release_type` can be `stable` (default) or `beta`.</string>
	<key>Identifier</key>
	<string>com.github.jps3.download.Cura</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Cura</string>
		<key>release_type</key>
		<string>stable</string>
		<key>update_url_stub</key>
		<string>http://software.ultimaker.com</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.4.0</string>
	<key>Process</key>
	<array>
        <dict>
            <key>Processor</key>
            <string>CuraUrlProvider</string>
        </dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<!-- If `RELEASE_TYPE` == `BETA` we should rename downloaded file to append 'BETA' somewhere, no? -->
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
